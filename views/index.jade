    script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js')
    script.
      $(document).ready(function() {
        $('ul.main_menu li').each(function() {
          $(this).click(function() {
            $(this).addClass('main_menu_selected');
            $(this).siblings().removeClass('main_menu_selected');

            var menu_child = $(this).children()[0];
            console.log(menu_child);

            var menu_company = document.getElementById('menu_company');
            var menu_client = document.getElementById('menu_client');
            var menu_gallery = document.getElementById('menu_gallery');

            if(menu_child === menu_company) {
              $('.sub_menu.company').show();
              $('.sub_menu.client').hide();
              $('.sub_menu.gallery').hide();
            } else if(menu_child === menu_client) {
              $('.sub_menu.client').show();
              $('.sub_menu.company').hide();
              $('.sub_menu.gallery').hide();
            } else if(menu_child === menu_gallery) {
              $('.sub_menu.gallery').show();
              $('.sub_menu.company').hide();
              $('.sub_menu.client').hide();
            }
          });
          $('ul.sub_menu li').each(function() {
            $(this).click(function() {
              $(this).addClass('sub_menu_selected');
              $(this).siblings().removeClass('sub_menu_selected');
            });
          });
        });
        $('.unlink').click(function(e) {
          e.preventDefault();
        });
        $('.sidenav a').click(function() {
          document.getElementById("mySidenav").style.width = "0";
          document.getElementById("main").style.marginLeft= "0";
          document.body.style.backgroundColor = "#050505";
        });
      });
      function openNav() {
        document.getElementById("mySidenav").style.width = "250px";
        document.getElementById("main").style.marginLeft = "250px";
        document.body.style.backgroundColor = "#050505";
      }
      function closeNav() {
        document.getElementById("mySidenav").style.width = "0";
        document.getElementById("main").style.marginLeft= "0";
        document.body.style.backgroundColor = "#050505";
      }
