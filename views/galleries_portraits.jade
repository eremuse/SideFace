  script(src='/javascripts/material-photo-gallery.js')
  script.
    var elem = document.querySelector('.m-p-g');
    document.addEventListener('DOMContentLoaded', function() {
    var gallery = new MaterialPhotoGallery(elem);
    });
    $("#menu_company").click(function(){
      $(".sub_menu.company").show();
      $(".sub_menu.client").hide();
      $(".sub_menu.gallery").hide();
    });
    $("#menu_client").click(function(){
      $(".sub_menu.client").show();
      $(".sub_menu.company").hide();
      $(".sub_menu.gallery").hide();
    });
    $("#menu_gallery").click(function(){
      $(".sub_menu.gallery").show();
      $(".sub_menu.company").hide();
      $(".sub_menu.client").hide();
    });
    function openNav() {
      document.getElementById("mySidenav").style.width = "100%";
      document.getElementById("main").style.marginLeft = "250px";
      document.body.style.backgroundColor = "#050505";
    }
    function closeNav() {
      document.getElementById("mySidenav").style.width = "0";
      document.getElementById("main").style.marginLeft= "0";
      document.body.style.backgroundColor = "#050505";
    }
